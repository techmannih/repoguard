{
	"name": "repoguard",
	"description": "streamlining github management from command line",
	"version": "1.1.0",
	"bin": {
		"repoguard": "index.js"
	},
	"author": {
		"name": "Ankit Ydv"
	},
	"keywords": [
		"repoguard",
		"cli",
		"github",
		"management",
		"automation",
		"tool",
		"javascript",
		"repository"
	],
	"pkg": {
		"targets": [
			"node18-linux-x64",
			"node18-macos-x64",
			"node18-win-x64"
		],
		"outputPath": "dist"
	},
	"scripts": {
		"format": "prettier --write \"./**/*.{js,json}\"",
		"build": "pkg . --out-path dist"
	},
	"dependencies": {
		"chalk": "^4.1.2",
		"cli-handle-error": "^4.4.0",
		"cli-handle-unhandled": "^1.1.1",
		"cli-table3": "^0.6.5",
		"cli-welcome": "^2.2.3",
		"meow": "^9.0.0",
		"node-fetch": "^2.7.0",
		"prompts": "^2.4.2"
	},
	"devDependencies": {
		"prettier": "^3.3.2",
		"pkg": "^5.8.1"
	}
}
